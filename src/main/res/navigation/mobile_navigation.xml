<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.myapplication.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.myapplication.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/examFragment"
        android:name="com.example.myapplication.ui.exam.ExamFragment"
        android:label="Learn"
        tools:layout="@layout/exam_fragment" />
    <fragment
        android:id="@+id/timeUpFragment"
        android:name="com.example.myapplication.ui.timeup.TimeUpFragment"
        android:label="Time Up"
        tools:layout="@layout/time_up_fragment" />
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.myapplication.ui.result.ResultFragment"
        android:label="Result"
        tools:layout="@layout/result_fragment" />
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.myapplication.ui.test.TestFragment"
        android:label="Exam"
        tools:layout="@layout/test_fragment" />
    <fragment
        android:id="@+id/lv2Fragment"
        android:name="com.example.myapplication.ui.test.Lv2Fragment"
        android:label="Type II"
        tools:layout="@layout/lv2_fragment" />
    <fragment
        android:id="@+id/kiemtralaiFragment"
        android:name="com.example.myapplication.ui.notifications.KiemtralaiFragment"
        android:label="Review"
        tools:layout="@layout/kiemtralai_fragment" />

</navigation>